<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="Testi" match-catalog="MAIL"></table-filter>
	<table catalog="MAIL" name="Testi">
		<column name="ID" type="integer"></column>
		<column name="Testo" type="string"></column>
	</table>
</hibernate-reverse-engineering>