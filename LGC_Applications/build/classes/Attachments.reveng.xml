<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="Attachments" match-name="Allegati" />
	<table catalog="Attachments" name="Allegati">
		<column name="ID" type="integer"></column>
		<column name="DataInserimento" type="timestamp"></column>
		<column name="Descrizione" type="string"></column>
		<column name="Allegato" type="binary"></column>
		<column name="CodiceFiscale" type="string"></column>
		<column name="ContentType" type="string"></column>
		<column name="Format" type="string"></column>
	</table>
</hibernate-reverse-engineering>